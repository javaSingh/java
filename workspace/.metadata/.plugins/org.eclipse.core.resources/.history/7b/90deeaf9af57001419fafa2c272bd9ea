<script>
$(document).ready(function(){
	  $('#newdoc').hide();
	  $('#senddoc').hide();
	}); 
function newdoc()
{
	$('#newdoc').toggle();
}
function senddoc()
{
	$('#senddoc').toggle();
}
</script>
